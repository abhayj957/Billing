    //dropdown menu for payment
    $(document).ready(function () {
        $(window).load(function () {
            var Payment = $("#ModeOfPayment");
            var Payment1 = $("#ModeOfPayment1");
            Payment.empty().append('<option selected="selected" value="0" disabled = "disabled">Loading.....</option>');
            Payment1.empty().append('<option selected="selected" value="0" disabled = "disabled">Loading.....</option>');
            $.ajax({
                url: "/POMaster/GetPaymentMode",
                type: "Get",
                success: function (response) {

                    Payment.empty().append('<option selected="selected" value="0">Select</option>');
                    Payment1.empty().append('<option selected="selected" value="0">Select</option>');
                    $.each(response, function () {
                        Payment.append($("<option></option>").val(this['Payment']).html(this['Payment']));
                        Payment1.append($("<option></option>").val(this['Payment']).html(this['Payment']));
                    });
                }
            });
        });
    });


    //dropdown menu for pocontact
    $(document).ready(function () {
        $(window).load(function () {
            var PointOfContact = $("#PointOfContact");
            var PointOfContact1 = $("#PointOfContact1");
            PointOfContact.empty().append('<option selected="selected" value="0" disabled = "disabled">Loading.....</option>');
            PointOfContact.empty().append('<option selected="selected" value="0" disabled = "disabled">Loading.....</option>');
            $.ajax({
                url: "/POMaster/GetPOC",
                type: "Get",
                success: function (response) {

                    PointOfContact.empty().append('<option selected="selected" value="0">Select</option>');
                    PointOfContact1.empty().append('<option selected="selected" value="0">Select</option>');
                    $.each(response, function () {
                        PointOfContact.append($("<option></option>").val(this['Point_Of_Contact']).html(this['Point_Of_Contact']));
                        PointOfContact1.append($("<option></option>").val(this['Point_Of_Contact']).html(this['Point_Of_Contact']));
                    });
                }
            });
        });
    });

    //searching box and paging
    $(document).ready(function () {
        $('#demoGrid').DataTable({
            "scrollY": 300,
            "scrollX": true,
            "lengthMenu": [[5, 10, 15, 20, 25, 30, 35, 40, 45, 50, - 1], [5, 10, 15, 20, 25, 30, 35, 40, 45, 50, "All"]],
            "pagingType": "full_numbers",
            "ordering":false
        }
        );
    });
    $('#demoGrid').dataTable({});
</script>

